<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER_LEFT" maxHeight="-Infinity" prefHeight="60.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.kruczek.mp3player.controller.ControlPaneController">
   <children>
      <Button id="prev" fx:id="prevButton" maxHeight="1.7976931348623157E308" mnemonicParsing="false" prefHeight="30.0" prefWidth="30.0" HBox.hgrow="NEVER">
         <HBox.margin>
            <Insets bottom="5.0" left="5.0" top="5.0" />
         </HBox.margin></Button>
      <ToggleButton id="play" fx:id="playButton" maxHeight="1.7976931348623157E308" mnemonicParsing="false" prefHeight="30.0" prefWidth="30.0" HBox.hgrow="NEVER">
         <HBox.margin>
            <Insets bottom="5.0" top="5.0" />
         </HBox.margin></ToggleButton>
      <Button id="next" fx:id="nextButton" maxHeight="1.7976931348623157E308" mnemonicParsing="false" prefHeight="30.0" prefWidth="30.0" HBox.hgrow="NEVER">
         <HBox.margin>
            <Insets bottom="5.0" top="5.0" />
         </HBox.margin></Button>
      <Slider fx:id="volumeSlider" maxHeight="-Infinity" orientation="VERTICAL" prefHeight="80.0" prefWidth="13.0" HBox.hgrow="NEVER">
         <HBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </HBox.margin></Slider>
      <Slider fx:id="songSlider" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </HBox.margin></Slider>
   </children>
</HBox>
